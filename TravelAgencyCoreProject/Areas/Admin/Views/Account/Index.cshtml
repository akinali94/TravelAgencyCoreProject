@model AccountViewModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Unit Of Work Personel Money Transfer</h3>

<form method="post">
    <label>Sender</label>
    <input type="text" asp-for="SenderID" class="form-control" />
    <br />
    <label>Receiver</label>
    <input type="text" asp-for="ReceiverID" class="form-control" />
    <br />
    <label>Amount</label>
    <input type="text" asp-for="Amount" class="form-control" />
    <br />
    <button class="btn btn-outline-warning"> Confirm</button>
</form>